<!doctype html>
<html lang="">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% block title %}
    <title>Studio Legale Avvocato Lenzi</title>
    {% endblock %}
    {% block description %}
    <meta name="description" content="">
    {% endblock %}

    {% block styles %}
    <link rel="stylesheet" href="styles/main.css">
    <link href="https://fonts.googleapis.com/css?family=Niconne|PT+Serif|PT+Sans:400,700|PT+Serif+Caption" rel="stylesheet">
    {% endblock %}
  </head>
  <body>
    {% block header %}
        {% include "app/includes/header.nj" %}
    {% endblock %}

    {% block content %}{% endblock %}

    {% block footer %}
        {% include "app/includes/footer.nj" %}
    {% endblock %}

    {% block scripts %}
    <script src="scripts/main.js"></script>
    {% endblock %}
  </body>
</html>
